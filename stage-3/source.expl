begin
    read(a);
    read(b);
    read(c);
    if (a < b) then
        if (b < c) then write(c); else write(b); endif;
    else
        if (a < c) then write(c); else write(a); endif;
    endif;

    read(n);
    i = 0;
    r = 0;
    while (i < n)  do read(d); if (d > r) then r = d; endif; i = i + 1; endwhile;
    write(r);
end

